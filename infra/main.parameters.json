{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "resourceGroupName": {
      "value": "rg-${AZURE_ENV_NAME}"
    },
    "bingApiKey": {
      "value": "${BING_API_KEY}"
    },
    "openaiApiKey": {
      "value": "${OPENAI_API_KEY}"
    },
    "openWeatherMapApiKey": {
      "value": "${OPENWEATHERMAP_API_KEY}"
    },
    "alphaVantageApiKey": {
      "value": "${ALPHA_VANTAGE_API_KEY}"
    },
    "finnhubApiKey": {
      "value": "${FINNHUB_API_KEY}"
    },
    "stripeSecretKey": {
      "value": "${STRIPE_SECRET_KEY}"
    },
    "stripePublishableKey": {
      "value": "${NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY}"
    },
    "firebaseApiKey": {
      "value": "${NEXT_PUBLIC_FIREBASE_API_KEY}"
    },
    "firebaseAuthDomain": {
      "value": "${NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN}"
    },
    "firebaseProjectId": {
      "value": "${NEXT_PUBLIC_FIREBASE_PROJECT_ID}"
    },
    "firebaseStorageBucket": {
      "value": "${NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET}"
    },
    "firebaseMessagingSenderId": {
      "value": "${NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID}"
    },
    "firebaseAppId": {
      "value": "${NEXT_PUBLIC_FIREBASE_APP_ID}"
    },
    "firebaseMeasurementId": {
      "value": "${NEXT_PUBLIC_FIREBASE_MEASUREMENT_ID}"
    }
  }
}
